		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.16.0",
		"flowbite-svelte-icons": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sidebar-menu": "^0.9.12",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
